@using Carlton.Dashboard.ViewModels.Feed

@inherits DashboardBaseComponent<FeedListViewModel>

<div class="feed-list-card">
    <CarltonListCard TItem="FeedListItemViewModel"
                     Items="ViewModel.FeedItems"
                     CardTitle="Feed">

        <MenuCardContent>
            <DashboardCardMenu TViewModel="FeedListViewModel" Callback="OnComponentEvent" Status="ComponentStatus" />
        </MenuCardContent>

        <ItemTemplate Context="feedItem">
            <FeedListItem ViewModel="feedItem" />
        </ItemTemplate>
    </CarltonListCard>
</div>


@code{
    public int x { get; set; }
}

