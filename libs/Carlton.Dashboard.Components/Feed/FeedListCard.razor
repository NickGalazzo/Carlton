
@inherits DashboardBaseComponent<FeedListViewModel>

<CarltonCssWraper ComponentType="GetType()">
    <Content>
        <CarltonListCard Items="ViewModel.FeedItems"
                         CardTitle="Feed">

            <MenuCardContent>
                <DashboardCardMenu TViewModel="FeedListViewModel" Callback="OnComponentEvent" Status="ComponentStatus" />
            </MenuCardContent>

            <ItemTemplate Context="feedItem">
                <FeedListItem ViewModel="feedItem" />
            </ItemTemplate>
        </CarltonListCard>
    </Content>
</CarltonCssWraper>



