
@inherits DashboardBaseComponent<FeedListViewModel>

<CarltonCssWraper ComponentType="GetType()">
    <Content>
        <CarltonListCard Items="ViewModel.FeedItems"
                         CardTitle="Feed">

            <ActionBarContent>
                <DashboardCardActionBar TViewModel="FeedListViewModel" 
                                        Callback="OnComponentEvent" 
                                        Status="ComponentStatus" />
            </ActionBarContent>

            <ItemTemplate Context="feedItem">
                <FeedListItem ViewModel="feedItem" />
            </ItemTemplate>
        </CarltonListCard>
    </Content>
</CarltonCssWraper>



