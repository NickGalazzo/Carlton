        <DashboardCardActionBar OnComponentEvent="base.OnComponentEvent" ComponentStatus="base.ComponentStatus" />
    </ActionBarContent>

    <ItemTemplate Context="feedItem">
        <FeedListItem ViewModel="feedItem" />
    </ItemTemplate>
</ListCard>



